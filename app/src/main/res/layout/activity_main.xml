<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yisingle.study.map.one.MainActivity">


    <TextView
        android:id="@+id/tvInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="test"
        android:text="点我验证是否集成成功"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btToNavi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="多路径规划"
        app:layout_constraintLeft_toLeftOf="parent"
        android:onClick="toNavi"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btCheck" />

    <Button
        android:id="@+id/btToNaviRightNow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="立即导航"
        app:layout_constraintLeft_toLeftOf="parent"
        android:onClick="toNaviRightNow"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btToNavi" />

</android.support.constraint.ConstraintLayout>
